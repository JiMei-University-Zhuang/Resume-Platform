{
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"],
  "compilerOptions": {
    "types": ["vite/client"],
    "isolatedModules": true,
    "lib": ["ES2017", "DOM"],
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "vueCompilerOptions": {
    "target": 3
  }
}
